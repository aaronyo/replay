<% for track in @tracks: %>
<div class="track">
  <img class="track-cover" src="<%- track.cover_url %>" />
  <div class="track-info">
    <div class="artist"><%= track.artist %></div>
    <div class="album-title"> <%= track.album_title %></div>
    <a class="track-title" id="<%= track.track_key %>" href="#"><%= track.track_number %>. <%= track.track_title %></a>
  </div>
</div>
<% end %>